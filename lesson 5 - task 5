def sum_new_file():
    with open("my_file5.txt", "w+", encoding='utf8') as my_file5:
        while True:
            inp = input("Здесь пользователь вводит числа, разделённые пробелами: ")
            my_file5.write(f" {inp}")
            if inp == "":
                break
    with open("my_file5.txt", "r", encoding='utf8') as my_file5:
        data = my_file5.read().split()
        i = 0
        for el in data:
            i = i + int(el)
        return print(i)
print(sum_new_file())
