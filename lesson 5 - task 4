def avg_salary():
    with open("text_4.txt", "r", encoding='utf8') as text_4:
        data = text_4.readlines()
        x = ['Один', 'Два', 'Три', 'Четыре']
        y = []
        for el in data:
            y.append((el.split(" - ")[1].strip()))
        dic = dict(zip(x, y))
        with open("text_4_2.txt", 'w', encoding='utf8') as text_4_2:
            text_4_2.write(str(dic))
        return print(dic)
print(avg_salary())

