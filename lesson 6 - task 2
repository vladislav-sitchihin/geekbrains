class Road:
    def __init__(self, length, width):
        self._length = length
        self._width = width

    def function(self):
        result = (self._length * self._width * 25 * 5) / 1000
        return f"Масса асфальтового покрытия составляет: {result} т"


Moskovskaya = Road(3000, 15)
print(Moskovskaya.function())


